  "devDependencies": {
    "nodemon": "^1.18.3"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2"
  }
